DEF Mill_LEFT_Loop() 
INT counter 

FOR counter = 1 TO 11593 
     ChangeSpeed(gsMill_LEFT1[counter].grfeedrate)
     LIN gsMill_LEFT1[counter].fCNC C_VEL 
ENDFOR 

END 
 
 
DEF ChangeSpeed(lrFeedRate:IN) 
     REAL lrFeedRate 
 
 
     LDAT_ACT.VEL = lrFeedRate * (DEF_VEL_CP / (DEF_VEL_CP*1000)) 
 
        IF (lrFeedRate < 1.0) THEN 
             PDAT_ACT.VEL = 1 
             PDAT_ACT.VEL = lrFeedRate 
        ENDIF 
 
     BAS(#CP_DAT) 
     BAS(#PTP_DAT) 

END 
 
 
